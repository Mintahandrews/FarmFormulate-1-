# AgriNova AI - Environment Variables

# API Configuration
VITE_API_URL=https://api.agrinovaai.com/v1
VITE_API_KEY=your_api_key_here

# Feature Flags
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_LOCATION_SERVICES=true
VITE_ENABLE_OFFLINE_MODE=true

# Localization
VITE_DEFAULT_LANGUAGE=en
VITE_SUPPORTED_LANGUAGES=en,tw,ew,ga,da,ha

# Limits
VITE_MAX_FORM_FIELDS=50
VITE_MAX_FORM_SIZE=500KB

# Analytics
VITE_ANALYTICS_ID=UA-123456789-1
